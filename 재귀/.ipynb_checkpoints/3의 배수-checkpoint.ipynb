   "source": [
    "n = input()\n",
    "cnt =0\n",
    "while len(n) !=1:\n",
    "    cnt+=1\n",
    "    a = sum([int(i) for i in n])\n",
    "    n = str(a)\n",
    "\n",
    "print(cnt)\n",
    "if int(n)%3 ==0:\n",
    "    print('YES')\n",
    "else:\n",
    "    print('NO')"
   ]
