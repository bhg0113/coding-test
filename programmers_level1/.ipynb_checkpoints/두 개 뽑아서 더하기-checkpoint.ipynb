   "source": [
    "def solution(numbers):\n",
    "    result = []\n",
    "    for i in range(len(numbers)):\n",
    "        for j in range(len(numbers)):\n",
    "            if i !=j:\n",
    "                result.append(numbers[i]+numbers[j])\n",
    "    return sorted(list(set(result)))"
   ]
