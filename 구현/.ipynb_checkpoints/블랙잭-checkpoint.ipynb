   ],
   "source": [
    "from itertools import combinations\n",
    "comb = combinations(cards,3)\n",
    "\n",
    "res = 0\n",
    "for c in list(comb):\n",
    "    if sum(c) <= m and m-sum(c)<m-res:\n",
    "        res = sum(c)\n",
    "print(res)\n",
    "\n",
    "# 삼중 반복문으로 풀이할 수 있음"
   ]
